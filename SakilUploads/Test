#include <iostream>

using namespace std;

int main()
{

int x,y;
  for (x = 0; x < 5; x++){
      cout<<"\n";
      for (y = 5; y > x; y--){
          cout<<"*";
        }
    }
  for (x = 0; x < 5; x++){
      cout<<"\n";
      for (y = -1; y < x; y++){
          cout<<"*";
        }
    }
    
return 0;
}


